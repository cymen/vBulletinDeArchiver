{
  "name": "vBulletin DeArchiver",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Automatically redirect to non-archive version of vBulletin content",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": [ "redirect_to_non_archive.js" ]
    }
  ],
  "permissions": [ "http://*/*", "https://*/*" ]
}
